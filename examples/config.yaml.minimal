mqtt:
  host: ${oc.env:MQTT_HOST}
  port: ${oc.env:MQTT_PORT}
  user: ${oc.env:MQTT_USER}
  password: ${oc.env:MQTT_PASS}

docker:
  enabled: True
  compose_version: v2

node:
  name: dockernuc
  healthcheck:
    interval: 300

homeassistant:
  discovery:
    prefix: homeassistant
    enabled: True

log:
  level: INFO
